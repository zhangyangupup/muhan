html, body, div, span, object,h1, h2, h3, h4, h5, h6, p,
a,b, u, i,dl, dt, dd, ol, ul, li,input
  margin: 0
  padding: 0
  border: 0
  font-size: 100%
  font-weight: normal
  vertical-align: baseline
  touch-action none
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif
input
  font-size 0.9rem
button
  width 100%
  height 2.2rem
  line-height 2.2rem
  border 0
  border-radius 5px
  color #fff
  outline none
button:active
  color #bbb
  animation: all 1s
body
  line-height 1
  .el-message
    min-width 60vw
    max-width 90vw
ul 
  list-style none
ul,p{
    list-style-type: none;
    margin-block-start: 0em;
    margin-block-end: 0em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 0px;
}
p{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}